{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^6.4",
        "laravel/passport": "^8.4",
        "laravel/tinker": "^2.0",
        "dingo/api": "^2.4.0"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpmd/phpmd": "^2.8",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": [
            "@composer install",
            "vendor/bin/phpunit -c phpunit.xml --colors=always"
        ],
        "cs:check": [
            "./vendor/bin/php-cs-fixer --dry-run fix app",
            "./vendor/bin/php-cs-fixer --dry-run fix tests",
            "./vendor/bin/php-cs-fixer --dry-run fix routes",
            "./vendor/bin/php-cs-fixer --dry-run fix config",
            "./vendor/bin/phpmd app text phpmd.xml"
        ],
        "cs:fix": [
            "./vendor/bin/php-cs-fixer fix app",
            "./vendor/bin/php-cs-fixer fix tests",
            "./vendor/bin/php-cs-fixer fix routes",
            "./vendor/bin/php-cs-fixer fix config"
        ],
        "pipeline:checks": [
            "@composer syntax:check",
            "@composer test"
        ]
    }
}
